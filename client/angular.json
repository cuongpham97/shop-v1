{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "administration": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/administration",
      "sourceRoot": "projects/administration/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/administration",
            "index": "projects/administration/src/index.html",
            "main": "projects/administration/src/main.ts",
            "polyfills": "projects/administration/src/polyfills.ts",
            "tsConfig": "projects/administration/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/administration/src/favicon.ico",
              "projects/administration/src/assets"
            ],
            "styles": [
              "projects/administration/src/app/scss/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/administration/src/environments/environment.ts",
                  "with": "projects/administration/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "administration:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "administration:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "administration:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/administration/src/test.ts",
            "polyfills": "projects/administration/src/polyfills.ts",
            "tsConfig": "projects/administration/tsconfig.spec.json",
            "karmaConfig": "projects/administration/karma.conf.js",
            "assets": [
              "projects/administration/src/favicon.ico",
              "projects/administration/src/assets"
            ],
            "styles": [
              "projects/administration/src/app/scss/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/administration/tsconfig.app.json",
              "projects/administration/tsconfig.spec.json",
              "projects/administration/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/administration/e2e/protractor.conf.js",
            "devServerTarget": "administration:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "administration:serve:production"
            }
          }
        }
      }
    },
    "store-front": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/store-front",
      "sourceRoot": "projects/store-front/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/store-front",
            "index": "projects/store-front/src/index.html",
            "main": "projects/store-front/src/main.ts",
            "polyfills": "projects/store-front/src/polyfills.ts",
            "tsConfig": "projects/store-front/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/store-front/src/favicon.ico",
              "projects/store-front/src/assets"
            ],
            "styles": [
              "projects/store-front/src/scss/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/store-front/src/environments/environment.ts",
                  "with": "projects/store-front/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "store-front:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "store-front:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "store-front:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/store-front/src/test.ts",
            "polyfills": "projects/store-front/src/polyfills.ts",
            "tsConfig": "projects/store-front/tsconfig.spec.json",
            "karmaConfig": "projects/store-front/karma.conf.js",
            "assets": [
              "projects/store-front/src/favicon.ico",
              "projects/store-front/src/assets"
            ],
            "styles": [
              "projects/store-front/src/scss/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/store-front/tsconfig.app.json",
              "projects/store-front/tsconfig.spec.json",
              "projects/store-front/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/store-front/e2e/protractor.conf.js",
            "devServerTarget": "store-front:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "store-front:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "store-front"
}
